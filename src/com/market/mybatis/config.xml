<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration
  PUBLIC "-//mybatis.org//DTD Config 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-config.dtd">
<configuration>
	<typeAliases>
		<typeAlias type="com.market.domain.BookmarkProduct" alias="BookmarkProduct"/>
		<typeAlias type="com.market.domain.Category" alias="Category"/>
		<typeAlias type="com.market.domain.Member" alias="Member"/>
		<typeAlias type="com.market.domain.OrderDetail" alias="OrderDetail"/>
		<typeAlias type="com.market.domain.Product" alias="Product"/>
		<typeAlias type="com.market.domain.ProductImage" alias="ProductImage"/>
		<typeAlias type="com.market.domain.State" alias="State"/>
		<typeAlias type="com.market.domain.Message" alias="Message"/>
	</typeAliases>
	<mappers>
		<mapper resource="com/market/mybatis/BookmarkProductMapper.xml"/>
		<mapper resource="com/market/mybatis/CategoryMapper.xml"/>
		<mapper resource="com/market/mybatis/MemberMapper.xml"/>
		<mapper resource="com/market/mybatis/OrderDetailMapper.xml"/>
		<mapper resource="com/market/mybatis/ProductMapper.xml"/>
		<mapper resource="com/market/mybatis/ProductImageMapper.xml"/>
		<mapper resource="com/market/mybatis/StateMapper.xml"/>
		<mapper resource="com/market/mybatis/MessageMapper.xml"/>
	</mappers>
	
</configuration>







